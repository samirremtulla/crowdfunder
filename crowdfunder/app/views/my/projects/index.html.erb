<h1>Welcome <%=current_user.email%></h1>
<%= link_to "New Project", new_my_project_path, :class => "btn btn-primary"%>
<ul>
<% @projects.each do |p|%>
  <p>
    <li><strong><%=p.title%></strong><br/>
    <%=p.teaser%></li>
    <%= link_to "Edit", edit_my_project_path(p)%>
  </p>
<%end%>
</ul>
<%= paginate @projects %>
